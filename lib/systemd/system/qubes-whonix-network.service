[Unit]
Description = Qubes Whonix network proxy setup
Before = network.target qubes-whonix-firewall
After = iptables.service

[Service]
Type = oneshot
RemainAfterExit = yes
ExecStart = /usr/lib/qubes-whonix/init/network-proxy-setup
ExecStartPost = /usr/lib/qubes-whonix/init/init
StandardOutput = syslog

[Install]
WantedBy = multi-user.target
Alias = qubes-network.service
